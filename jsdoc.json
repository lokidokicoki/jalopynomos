{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "templates": {
    "cleverLinks": true,
    "theme": "yeti",
    "systemName": "Jalopynomos",
    "includeDate": true
  },
  "opts": {
    "destination": "./docs/",
    "private": true
  },
  "source": {
    "exclude": ["src/public"]
  }
}
